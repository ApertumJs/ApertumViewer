{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "warn", 
        "no-use-before-define": "warn", 
        "spaced-comment": "warn",
        "linebreak-style": 0,
        "import/no-unresolved": 0,
        "no-undef":0,
        "import/extensions": 0,
        "no-unused-vars": 0
    }
}
